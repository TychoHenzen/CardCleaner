config_version=5

[application]

config/name="CardCleaner"
run/main_scene="res://Scenes/StartScene.tscn"
config/features=PackedStringArray("4.4", "C#", "Forward Plus")
config/icon="res://icon.svg"
config/defaults/default_clear_color=Color(0.3, 0.3, 0.3, 1)

[dotnet]

project/assembly_name="CardCleaner"

[editor_plugins]

enabled=PackedStringArray("res://addons/contextDumper/plugin.cfg")

[physics]

3d/run_on_separate_thread=true
3d/physics_engine="Jolt Physics"
3d/solver/solver_iterations=32
